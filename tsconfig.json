{
	"compilerOptions": {
		"target": "es5",
		"moduleResolution": "node",
		"strict": true,
		"esModuleInterop": true,
		"noImplicitAny": true,
		"experimentalDecorators": true
	},
	"typedocOptions": {
		"mode": "modules",
		"module": "commonjs",
		"target": "ES6",
		"out": "docs",
		"exclude": [
			"src/**/index.ts"
		]
	}
}
